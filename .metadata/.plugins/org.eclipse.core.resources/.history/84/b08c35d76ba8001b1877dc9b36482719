package oopIntro;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
       
		//örnek olusturma, referance olusturma, instance olusturma
		Product pro1= new Product();
		pro1.id=1;
		pro1.name="lenova";
		pro1.unitPrice=1200;
		pro1.detail="16 gb ram";
		Product pro2= new Product();
		pro2.id=2;
		pro2.name="hp";
		pro2.unitPrice=12000;
		pro2.detail="16 gb ram";
		Product pro3= new Product(3,"lenova2",123,"16 gb ram");
		
		
		Product[] products= {pro1,pro2,pro3};
		
		for(Product product:products) {
			System.out.println(product.name);
		}
		System.out.println(products.length);
	
		Category category1=new Category();
		category1.id=1;
		category1.name="Ev/Bahçe";
		Category category2=new Category();
		category2.id=2;
		category2.name="Eşya";
		
		ProductManager productManager=new ProductManager();
		productManager.addToCart(pro1);
	}

}
